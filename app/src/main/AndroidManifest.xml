<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.agima.mymoxyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".ui.samples.Application"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.samples.fSimple.FSimpleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.samples.sSimple.SSimpleActivity" />
        <activity android:name=".ui.samples.list.ListScreenActivity" />
        <activity android:name=".ui.samples.presenterList.PresenteredListActivity" />
    </application>


</manifest>
